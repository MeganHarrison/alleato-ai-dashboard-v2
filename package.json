{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ts": "concurrently \"tsc --noEmit --watch --preserveWatchOutput\" \"next dev\"",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:check": "npm run typecheck && npm run pre-deploy && npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "type:errors": "tsc --noEmit | head -20",
    "type:count": "tsc --noEmit 2>&1 | wc -l",
    "type:fix": "npm run typecheck || true",
    "pre-deploy": "node scripts/pre-deploy-check.js",
    "quick-check": "node scripts/quick-check.js",
    "update-types": "npx supabase gen types typescript --project-id rpynhlesifkxqkokctmw > types/database.types.ts",
    "test:vector": "tsx tests/test-vector-search.ts",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:pm": "playwright test tests/e2e/pm-assistant.spec.ts",
    "mcp:start": "node scripts/run-mcp-server.js",
    "mcp:install": "pnpm add -D @supabase/mcp-server-supabase",
    "dev:with-mcp": "concurrently \"npm run mcp:start\" \"npm run dev\"",
    "validate": "npm run lint && npm run test && npx playwright test",
    "setup:rag": "node scripts/setup-rag-system.js",
    "sync:fireflies": "node scripts/fireflies-sync-to-documents.js",
    "sync:fireflies-meetings": "node scripts/fireflies-sync-to-meetings-table.js",
    "sync:fireflies-complete": "node scripts/fireflies-sync-to-documents-complete.js",
