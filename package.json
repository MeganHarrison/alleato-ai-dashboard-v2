    "sync:fireflies-v2": "node scripts/enhanced-fireflies-sync-v2.js",
    "check:meetings-table": "node scripts/check-meetings-table.js",
    "vectorize:batch": "node scripts/vectorize-batch.js",
    "vectorize:all": "node scripts/vectorize-all-documents.js",
    "prepare": "husky",
    "clean": "rm -rf test-results screenshots coverage .next dist",
    "clean:all": "npm run clean && rm -rf node_modules .pnpm-store",
    "clean:test": "rm -rf test-results screenshots playwright-report",
    "organize:check": "sh .husky/pre-commit"
